{
  "name": "norsk-studio-starter-kit",
  "version": "1.0.6",
  "description": "",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npx studio-wrap-infos src/ && npx npm-run-all build:* && rm -rf build",
    "build-docker": "npm run build && ./scripts/build-docker.sh",
    "_build:eslint": "npx eslint src",
    "build:server": "npx tsc",
    "build:static": "npx tsc -p tsconfig-static.json && npx studio-bundle build/info.js static/info.js",
    "build:css": "rsync --mkpath -v -r --include '*/' --include '*.css' --exclude '*' src/ static/",
    "server": "npx studio-editor",
    "test": "echo nothing to see here yet",
    "release": "./release.sh"
  },
  "files": [
    "static/*",
    "lib/*",
    "config/*",
    "mnt/*"
  ],
  "main": "lib/index.js",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@norskvideo/norsk-sdk": "^1.0.376",
    "@norskvideo/norsk-studio": "file:../norsk-studio-core",
    "@norskvideo/norsk-studio-aws": "file:../norsk-studio/workspaces/aws",
    "@norskvideo/norsk-studio-built-ins": "file:../norsk-studio/workspaces/built-ins",
    "@norskvideo/norsk-studio-source-switcher": "file:../norsk-studio/workspaces/source-switcher",
    "norsk-studio-starter-kit": "file:."
  },
  "devDependencies": {
    "@norskvideo/webrtc-client": "^0.1.2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "esbuild": "^0.21.0",
    "esbuild-plugin-external-global": "^1.0.1",
    "eslint": "^8.53.0",
    "eventhandler": "^2.1.0",
    "http-server": "^14.1.1",
    "isomorphic-ws": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-jsx": "^1.0.0",
    "typescript": "^5.4.5"
  }
}
